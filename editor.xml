<application>
  <component name="DaemonCodeAnalyzerSettings" profile="Project Default" />
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Cascadia Code" />
    <option name="LINE_SPACING" value="1.4" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>