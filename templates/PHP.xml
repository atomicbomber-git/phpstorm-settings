<templateSet group="PHP">
  <template name="dd400" value="http_response_code(400);&#10;dd($$$var$);" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP Expression" value="true" />
      <option name="PHP Statement" value="true" />
    </context>
  </template>
  <template name="btext" value="&lt;div class=&quot;form-group&quot;&gt;&#10;                &lt;label for=&quot;judul&quot;&gt;&#10;                    Label:&#10;                &lt;/label&gt;&#10;                &lt;textarea&#10;                    class=&quot;form-control&quot;&#10;                    name=&quot;judul&quot;&#10;                    id=&quot;judul&quot;&#10;                    cols=&quot;30&quot;&#10;                    rows=&quot;10&quot;&gt;&lt;/textarea&gt;&#10;            &lt;/div&gt;" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
