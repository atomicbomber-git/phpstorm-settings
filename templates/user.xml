<templateSet group="user">
  <template name="bulinput" value="&lt;div class=&quot;field&quot;&gt;&#10;    &lt;label class=&quot;label&quot;&#10;           for=&quot;$FIELD$&quot;&#10;    &gt;$LABEL$&lt;/label&gt;&#10;    &lt;div class=&quot;control @error('$FIELD$') has-icons-right @enderror&quot;&gt;&#10;        &lt;input id=&quot;$FIELD$&quot;&#10;               name=&quot;$FIELD$&quot;&#10;               class=&quot;input @error('$FIELD$') is-danger @enderror&quot;&#10;               type=&quot;text&quot;&#10;               placeholder=&quot;$LABEL$&quot;&#10;               value=&quot;{{ old(&quot;$FIELD$&quot;)  }}&quot;&#10;        &gt;&#10;        @error('$FIELD$')&#10;        &lt;span class=&quot;icon is-small is-right&quot;&gt;&#10;                &lt;i class=&quot;fas fa-exclamation-triangle&quot;&gt;&lt;/i&gt;&#10;            &lt;/span&gt;&#10;        @enderror&#10;    &lt;/div&gt;&#10;    @error('$FIELD$')&#10;    &lt;p class=&quot;help is-danger&quot;&gt; {{ $message }} &lt;/p&gt;&#10;    @enderror&#10;&lt;/div&gt;&#10;$END$" description="Bulma Input" toReformat="true" toShortenFQNames="true">
    <variable name="FIELD" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="LABEL" expression="spaceSeparated(capitalize(camelCase(FIELD)))" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="bultext" value="&lt;div class=&quot;field&quot;&gt;&#10;    &lt;label class=&quot;label&quot;&#10;           for=&quot;$FIELD$&quot;&#10;    &gt;$LABEL$&lt;/label&gt;&#10;    &lt;div class=&quot;control @error('$FIELD$') has-icons-right @enderror&quot;&gt;&#10;        &lt;textarea id=&quot;$FIELD$&quot;&#10;                  name=&quot;$FIELD$&quot;&#10;                  rows=&quot;5&quot;&#10;                  class=&quot;textarea @error('$FIELD$') is-danger @enderror&quot;&#10;                  placeholder=&quot;$LABEL$&quot;&#10;        &gt;{{ old(&quot;$FIELD$&quot;) }}&lt;/textarea&gt;&#10;        @error('$FIELD$')&#10;        &lt;span class=&quot;icon is-small is-right&quot;&gt;&#10;            &lt;i class=&quot;fas fa-exclamation-triangle&quot;&gt;&lt;/i&gt;&#10;        &lt;/span&gt;&#10;        @enderror&#10;    &lt;/div&gt;&#10;    @error('$FIELD$')&#10;    &lt;p class=&quot;help is-danger&quot;&gt; {{ $message }} &lt;/p&gt;&#10;    @enderror&#10;&lt;/div&gt;" description="Bulma Textarea" toReformat="true" toShortenFQNames="true">
    <variable name="FIELD" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="LABEL" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="bulfile" value="&lt;div class=&quot;field&quot;&gt;&#10;    &lt;label class=&quot;label&quot;&#10;           for=&quot;$FIELD$&quot;&#10;    &gt; Gambar &lt;/label&gt;&#10;&#10;    &lt;div class=&quot;file&quot;&gt;&#10;        &lt;label class=&quot;file-label&quot;&gt;&#10;            &lt;input class=&quot;file-input&quot; accept=&quot;$ACCEPT$&quot; type=&quot;file&quot; name=&quot;$FIELD$&quot;&gt;&#10;            &lt;span class=&quot;file-cta&quot;&gt;&#10;              &lt;span class=&quot;file-icon&quot;&gt;&#10;                &lt;i class=&quot;fas fa-upload&quot;&gt;&lt;/i&gt;&#10;              &lt;/span&gt;&#10;              &lt;span class=&quot;file-label&quot;&gt;&#10;                $INFO_TEXT$&#10;              &lt;/span&gt;&#10;            &lt;/span&gt;&#10;        &lt;/label&gt;&#10;    &lt;/div&gt;&#10;&#10;    @error('$FIELD$')&#10;    &lt;p class=&quot;help is-danger&quot;&gt; {{ $message }} &lt;/p&gt;&#10;    @enderror&#10;&lt;/div&gt;" description="Bulma File Input" toReformat="true" toShortenFQNames="true">
    <variable name="FIELD" expression="" defaultValue="image" alwaysStopAt="true" />
    <variable name="ACCEPT" expression="" defaultValue="image/png,image/jpeg,image/jpg" alwaysStopAt="true" />
    <variable name="INFO_TEXT" expression="" defaultValue="Pilih berkas gambar..." alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="bulbut" value="&lt;a href=&quot;#&quot; class=&quot;button is-primary is-small&quot;&gt;&#10;    &lt;span class=&quot;icon is-small&quot;&gt;&#10;        &lt;i class=&quot;fas fa-pencil-alt&quot;&gt;&lt;/i&gt;&#10;    &lt;/span&gt;&#10;    &lt;span&gt;&#10;        $CAPTION$&#10;    &lt;/span&gt;&#10;&lt;/a&gt;" description="Bulma Button" toReformat="true" toShortenFQNames="true">
    <variable name="CAPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="cinject" value="private ResponseFactory $responseFactory;&#10;&#10;public function __construct(ResponseFactory $responseFactory)&#10;{&#10;&#10;    $this-&gt;responseFactory = $responseFactory;&#10;}" description="Laravel Controller Injection" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP Class Member" value="true" />
    </context>
  </template>
  <template name="belto" value="public function $FUNCTION_NAME$(): BelongsTo&#10;{&#10;    return $this-&gt;belongsTo($CLASS_NAME$::class);&#10;}" description="Laravel Belongs To Relationship" toReformat="true" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="FUNCTION_NAME" expression="snakeCase(CLASS_NAME)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="PHP Class Member" value="true" />
    </context>
  </template>
  <template name="retview" value="return $this-&gt;responseFactory-&gt;view(&quot;$VIEW_PATH$&quot;, [&#10;    $END$        &#10;]);" description="Laravel Controller Return View" toReformat="true" toShortenFQNames="true">
    <variable name="VIEW_PATH" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP Expression" value="true" />
      <option name="PHP Statement" value="true" />
    </context>
  </template>
  <template name="bcan" value="@can(\App\Providers\AuthServiceProvider::$RULE$)&#10;    $END$                         &#10;@endcan" description="Blade Can" toReformat="true" toShortenFQNames="true">
    <variable name="RULE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="bsection" value="@section(&quot;$SECTION_NAME$&quot;)&#10;    $END$&#10;@endsection" description="Blade Section" toReformat="true" toShortenFQNames="true">
    <variable name="SECTION_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TOP_LEVEL" value="true" />
    </context>
  </template>
  <template name="loopiter" value="{{ $loop-&gt;iteration }}" description="Blade Loop Iteration" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="bforeach" value="@foreach ($$$ITEM$s as $$$ITEM$)&#10;                                &#10;@endforeach" description="" toReformat="true" toShortenFQNames="true">
    <variable name="ITEM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="bif" value="@if($CONDITIONAL$)&#10;    $END$                         &#10;@endif" description="Blade If Conditional" toReformat="true" toShortenFQNames="true">
    <variable name="CONDITIONAL" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="bforelse" value="@forelse ($$$ITEM$s as $$$ITEM$)&#10;    $END$&#10;@empty&#10;    &lt;div class=&quot;alert alert-warning&quot;&gt;&#10;        &lt;i class=&quot;fas fa-exclamation-triangle&quot;&gt;&lt;/i&gt;&#10;        {{ __(&quot;messages.no_data&quot;) }}&#10;    &lt;/div&gt;&#10;@endforelse" description="Blade ForElse" toReformat="true" toShortenFQNames="true">
    <variable name="ITEM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="forem" value="@if($$$ITEM$s-&gt;isNotEmpty())&#10;    &lt;div class=&quot;table-responsive&quot;&gt;&#10;        &lt;table class=&quot;table table-sm table-striped table-hover&quot;&gt;&#10;            &lt;thead&gt;&#10;                &lt;tr&gt;&#10;                    &lt;th&gt; # &lt;/th&gt;&#10;                    $END$&#10;                &lt;/tr&gt;&#10;            &lt;/thead&gt;&#10;&#10;            &lt;tbody&gt;&#10;                @foreach ($$$ITEM$s as $$$ITEM$)&#10;&#10;                @endforeach&#10;            &lt;/tbody&gt;&#10;        &lt;/table&gt;&#10;    &lt;/div&gt;&#10;@else&#10;    &lt;div class=&quot;alert alert-warning&quot;&gt;&#10;        &lt;i class=&quot;fas fa-exclamation-triangle&quot;&gt;&lt;/i&gt;&#10;        {{ __(&quot;messages.no_data&quot;) }}&#10;    &lt;/div&gt;&#10;@endif" description="Blade ForEach Loop with Empty Guard" toReformat="true" toShortenFQNames="true">
    <variable name="ITEM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
</templateSet>