<templateSet group="user">
  <template name="phpforeach" value="&lt;?php foreach($$$items$ as $$$item$): ?&gt;&#10;                    $$&#10;                &lt;?php endforeach; ?&gt;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="items" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="item" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="phe" value="&lt;?= $$$var$ ?&gt;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var" expression="" defaultValue="" alwaysStopAt="true" />
