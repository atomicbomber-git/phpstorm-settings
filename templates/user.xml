<templateSet group="user">
  <template name="phpforeach" value="&lt;?php foreach($$$items$ as $$$item$): ?&gt;&#10;                    $$&#10;                &lt;?php endforeach; ?&gt;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="items" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="item" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="phe" value="&lt;?= $$$var$ ?&gt;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="var" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="VUE_INSIDE_TAG" value="true" />
    </context>
  </template>
  <template name="blade-section" value="@section(&quot;$section$&quot;)&#10;    $content$&#10;@endsection" description="Blade Section" toReformat="true" toShortenFQNames="true">
    <variable name="section" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="ukinput" value="&lt;div class=&quot;uk-margin&quot;&gt;&#10;    &lt;label class=&quot;uk-form-label&quot;&#10;           for=&quot;$field$&quot;&gt; $Label$&#10;    &lt;/label&gt;&#10;    &lt;div class=&quot;uk-form-controls&quot;&gt;&#10;        &lt;input&#10;            id=&quot;$field$&quot;&#10;            name=&quot;$field$&quot;&#10;            placeholder=&quot;$Label$&quot;&#10;            value=&quot;{{ old(&quot;$field$&quot;) }}&quot;&#10;            type=&quot;text&quot;&#10;            class=&quot;uk-input {{ $errors-&gt;has(&quot;$field$&quot;) ? &quot;uk-form-danger&quot; : &quot;&quot; }}&quot;&gt;&#10;&#10;        &lt;?php foreach($errors-&gt;get(&quot;$field$&quot;) as $error): ?&gt;&#10;            &lt;span class=&quot;uk-text-danger uk-text-small&quot;&gt;&#10;                {{ $error }}&#10;            &lt;/span&gt;&#10;        &lt;?php endforeach; ?&gt;&#10;    &lt;/div&gt;&#10;&lt;/div&gt;" description="UIKit Input" toReformat="true" toShortenFQNames="true">
    <variable name="field" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="Label" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="uktextarea" value="&lt;div class=&quot;uk-margin&quot;&gt;&#10;    &lt;label for=&quot;$field$&quot;&#10;           class=&quot;uk-form-label&quot;&gt; $Label$&#10;    &lt;/label&gt;&#10;&#10;    &lt;div class=&quot;uk-form-controls&quot;&gt;&#10;            &lt;textarea&#10;                id=&quot;$field$&quot;&#10;                rows=&quot;4&quot;&#10;                name=&quot;$field$&quot;&#10;                placeholder=&quot;$Label$&quot;&#10;                type=&quot;text&quot;&#10;                class=&quot;uk-textarea {{ $errors-&gt;has(&quot;$field$&quot;) ? &quot;uk-form-danger&quot; : &quot;&quot; }}&quot;&gt;{{ old(&quot;$field$&quot;) }}&lt;/textarea&gt;&#10;    &lt;/div&gt;&#10;&#10;    &lt;?php foreach($errors-&gt;get(&quot;$field$&quot;) as $error): ?&gt;&#10;        &lt;span class=&quot;uk-text-danger uk-text-small&quot;&gt;&#10;            {{ $error }}&#10;        &lt;/span&gt;&#10;    &lt;?php endforeach; ?&gt;&#10;&lt;/div&gt;" description="UIKit Textarea" toReformat="true" toShortenFQNames="true">
    <variable name="field" expression="" defaultValue="field" alwaysStopAt="true" />
    <variable name="Label" expression="" defaultValue="Label" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="ukformsubmit" value="&lt;div class=&quot;uk-margin uk-flex uk-flex-right&quot;&gt;&#10;    &lt;button class=&quot;uk-button uk-button-primary&quot;&#10;            type=&quot;submit&quot;&gt;&#10;        Submit&#10;    &lt;/button&gt;&#10;&lt;/div&gt;" description="UIKit Form Submit" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="vukinput" value="&lt;div class=&quot;uk-margin&quot;&gt;&#10;    &lt;label for=&quot;$field$&quot;&#10;           class=&quot;uk-form-label&quot;&gt; $Label$&#10;    &lt;/label&gt;&#10;&#10;    &lt;input &#10;        id=&quot;$field$&quot;&#10;        v-model=&quot;$field$&quot;&#10;        placeholder=&quot;$Label$&quot;&#10;        class=&quot;uk-input&quot;&#10;       :class=&quot;{&#10;            'uk-form-danger': !!this.get(error_data, 'errors.$field$[0]', false)&#10;        }&quot;&#10;       type=&quot;text&quot;&#10;          &gt;&#10;    &lt;span class=&quot;uk-text-danger uk-text-small&quot;&gt;&#10;        {{ this.get(error_data, 'errors.$field$[0]', '')}}&#10;    &lt;/span&gt;&#10;&lt;/div&gt;" description="UIKit Vue Input" toReformat="true" toShortenFQNames="true">
    <variable name="field" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="Label" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="mbmulti" value="&lt;multiselect&#10;        style=&quot;&quot;&#10;        placeholder=&quot;$Placeholder$&quot;&#10;        selectLabel=&quot;&quot;&#10;        selectedLabel=&quot;&quot;&#10;        deselectLabel=&quot;&quot;&#10;        track-by=&quot;id&quot;&#10;        label=&quot;$label$&quot;&#10;        :options=&quot;$field$s&quot;&#10;        v-model=&quot;$field$&quot;&#10;&gt;&#10;&lt;/multiselect&gt;" description="MB Multiselect" toReformat="true" toShortenFQNames="true">
    <variable name="Placeholder" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="label" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="field" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="mformat" value="moment($var$).format(&quot;YYYY-MM-DD HH:mm:ss&quot;)" description="Moment.js Format" toReformat="true" toShortenFQNames="true">
    <variable name="var" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_EXPRESSION" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="VUE_COMPONENT_DESCRIPTOR" value="true" />
    </context>
  </template>
  <template name="ajaxconf" value="modal.confirmationModal()&#10;    .then(result =&gt; {&#10;        if (!result.value) throw new Error();&#10;        return axios.post(this.submit_url, this.form_data)&#10;    })&#10;    .then(() =&gt; {&#10;        window.location.replace(this.redirect_url)&#10;    })&#10;    .catch(error =&gt; {&#10;        let error_data = get(error, &quot;response.data&quot;, null);&#10;        if (error_data) this.error_data = error_data;&#10;    })" description="Ajax Confirmation Modal" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="VUE_COMPONENT_DESCRIPTOR" value="true" />
    </context>
  </template>
  <template name="vaerror" value="{{ this.get(error_data, [`errors`, `$array_name$.${index}.$attribute_name$`, 0], '')}}" description="Get Array Error" toReformat="true" toShortenFQNames="true">
    <variable name="array_name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="attribute_name" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
</templateSet>