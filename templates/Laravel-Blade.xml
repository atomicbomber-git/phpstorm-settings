    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="@while" value="@while ($START$)&#10;    $END$&#10;@endwhile" description="Blade @while" toReformat="false" toShortenFQNames="true">
